<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="food_delivery" match-name="dishes"/>
  <table-filter match-catalog="food_delivery" match-name="orders"/>
  <table-filter match-catalog="food_delivery" match-name="orders_dishes"/>
  <table-filter match-catalog="food_delivery" match-name="restaurant"/>
  <table-filter match-catalog="food_delivery" match-name="users"/>
</hibernate-reverse-engineering>